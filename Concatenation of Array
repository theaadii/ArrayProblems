class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) 
    {
        vector<int>ans(nums.size() + nums.size());
        int i=0;
        int j=0;
        int n = nums.size() + nums.size();
        while( i < n && j < nums.size()) 
        {
            ans[i] = nums[j];
            ans[i+ nums.size()] = nums[j];
            i++;
            j++;
        }
        return ans;
    }
};

